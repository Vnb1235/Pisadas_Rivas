#rmp_menu_trigger-1106 {
  width: 45px;
  height: 45px;
  position: fixed;
  top: 15px;
  border-radius: 10px;
  display: none;
  text-decoration: none;
  right: 5%;
  background: #ffffff;
  transition: transform 0.5s, background-color 0.5s;
}
#rmp_menu_trigger-1106:hover, #rmp_menu_trigger-1106:focus {
  background: #ffffff;
  text-decoration: unset;
}
#rmp_menu_trigger-1106.is-active {
  background: #ffffff;
}
#rmp_menu_trigger-1106 .rmp-trigger-box {
  width: 25px;
  color: #fff;
}
#rmp_menu_trigger-1106 .rmp-trigger-icon-active, #rmp_menu_trigger-1106 .rmp-trigger-text-open {
  display: none;
}
#rmp_menu_trigger-1106.is-active .rmp-trigger-icon-active, #rmp_menu_trigger-1106.is-active .rmp-trigger-text-open {
  display: inline;
}
#rmp_menu_trigger-1106.is-active .rmp-trigger-icon-inactive, #rmp_menu_trigger-1106.is-active .rmp-trigger-text {
  display: none;
}
#rmp_menu_trigger-1106 .rmp-trigger-label {
  color: #fff;
  pointer-events: none;
  line-height: 13px;
  font-family: inherit;
  font-size: 14px;
  display: inline;
  text-transform: inherit;
}
#rmp_menu_trigger-1106 .rmp-trigger-label.rmp-trigger-label-top {
  display: block;
  margin-bottom: 12px;
}
#rmp_menu_trigger-1106 .rmp-trigger-label.rmp-trigger-label-bottom {
  display: block;
  margin-top: 12px;
}
#rmp_menu_trigger-1106 .responsive-menu-pro-inner {
  display: block;
}
#rmp_menu_trigger-1106 .rmp-trigger-icon-inactive .rmp-font-icon {
  color: #3f3f3f;
}
#rmp_menu_trigger-1106 .responsive-menu-pro-inner, #rmp_menu_trigger-1106 .responsive-menu-pro-inner::before, #rmp_menu_trigger-1106 .responsive-menu-pro-inner::after {
  width: 25px;
  height: 3px;
